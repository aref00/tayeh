{
  "name": "tayeh.js",
  "version": "0.1.9",
  "description": "tayeh shop-api client",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "test": "jest",
    "build": "microbundle",
    "dev": "microbundle watch",
    "package": "npm run test && npm run build && npm publish --public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PchasSoftware/tayeh.git"
  },
  "keywords": [
    "tayeh",
    "instance",
    "api"
  ],
  "author": "saeed.salehi",
  "contributors": [
    "saeed.salehi",
    "Aref Akbari"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PchasSoftware/tayeh/issues"
  },
  "homepage": "https://github.com/PchasSoftware/tayeh#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.8",
    "jest": "^26.4.2",
    "microbundle": "^0.12.3",
    "nock": "^13.0.4",
    "ts-jest": "^26.4.4"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "querystringify": "^2.2.0",
    "urix": "^0.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
